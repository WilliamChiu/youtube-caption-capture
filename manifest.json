{
  "name": "Youtube Caption Capture",
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "webRequest",
    "<all_urls>"
  ],
  "description": "While captions are on, click extension to view a video's captions in another tab.",
  "homepage_url": "https://chilly.blue",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
  },
  "background": {
    "page": "src/background/background.html",
    "persistent": true
  }
}